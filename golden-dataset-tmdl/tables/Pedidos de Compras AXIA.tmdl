	column STATUS_ATRASO =
			
			IF (
			    'Pedidos de Compras AXIA'[DATA_PREV_ENTREGA] < TODAY ()
			        && NOT (
			            'Pedidos de Compras AXIA'[STATUS_OBS_UNIFICADO]
			                IN { "Cancelado", "Concluído", "Resíduo" }
			        ),
			    "Atrasado",
			    "Não Atrasado"
			)
		lineageTag: 394a0d49-2c19-486e-9c0e-eb2139ba4467
		summarizeBy: none

		changedProperty = IsHidden

		annotation SummarizationSetBy = Automatic

	column DIAS_ATRASADO =
			
			IF (
			    'Pedidos de Compras AXIA'[DATA_PREV_ENTREGA] < TODAY ()
			        && NOT (
			            'Pedidos de Compras AXIA'[STATUS_OBS_UNIFICADO]
			                IN { "Cancelado", "Concluído", "Resíduo" }
			        ),
			    TODAY() - 'Pedidos de Compras AXIA'[DATA_PREV_ENTREGA],
			    0
			)
		lineageTag: 6a0e985d-cd1f-488d-a3bc-b8c5345c43bc
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

		annotation PBI_FormatHint = {"isGeneralNumber":true}

