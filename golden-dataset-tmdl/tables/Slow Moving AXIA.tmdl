		sourceColumn: Estoque Atual AXIA[CUSTO_TOTAL]
		columnOrigin: 'Estoque Atual AXIA'.CUSTO_TOTAL

		annotation SummarizationSetBy = Automatic

		annotation PBI_FormatHint = {"currencyCulture":"pt-BR"}

	column 'Data da Última Compra (Coluna)' =
			
			
			CALCULATE(
			    MAX('Entradas AXIA'[DATA_EMISSAO]),
			    FILTER(
			        'Entradas AXIA',
			        'Entradas AXIA'[ID_PRODUTO] = 'Slow Moving AXIA'[ID_PRODUTO]
			        && 'Entradas AXIA'[COD_FILIAL] = 'Slow Moving AXIA'[COD_FILIAL]
			    )
			)
		formatString: Short Date
		lineageTag: 1ee8ee41-df9b-4271-803d-a098c8aa59ec
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

		annotation UnderlyingDateTimeDataType = Date

	column 'Data da Última Venda (Coluna)' =
			
			CALCULATE(
			    MAX('Vendas Total AXIA'[DATA_EMISSAO]),
			    FILTER(
			        'Vendas Total AXIA',
			        'Vendas Total AXIA'[ID_PRODUTO] = 'Slow Moving AXIA'[ID_PRODUTO]
			        && 'Vendas Total AXIA'[CNPJ] = 'Slow Moving AXIA'[COD_FILIAL]
			    )
			)
		formatString: Short Date
		lineageTag: f0cff47c-a803-4d30-beb7-239a4eb218f1
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

		annotation UnderlyingDateTimeDataType = Date

	column 'Dias Sem Movimentação' =
			
			
			IF('Slow Moving AXIA'[Data da Última Compra (Coluna)]<1,0,MIN(TODAY()-'Slow Moving AXIA'[Data da Última Compra (Coluna)],today()-'Slow Moving AXIA'[Data da Última Venda (Coluna)]))
		formatString: 0
		lineageTag: 9c0982d1-5286-44d9-8970-16aa50770249
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

		annotation PBI_FormatHint = {"isDecimal":true}

	column Obsolescência =
			
			
			IF('Slow Moving AXIA'[Dias Sem Movimentação]<270,"Não Obsoleto","Obsoleto")
		lineageTag: 310b8e36-f560-4d65-aab7-c72fbc535555
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column DIO =
			
			
			ROUNDUP([DIO por Loja],0)
		formatString: 0
		lineageTag: 1fe3600d-5bee-4d21-985d-f9127d89eb9a
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

		annotation PBI_FormatHint = {"isDecimal":true}

	column 'Classificação Slow Moving' =
			
			
			IF('Slow Moving AXIA'[DIO]<180,"<180 dias",IF(AND('Slow Moving AXIA'[DIO]>180,'Slow Moving AXIA'[DIO]<360),"Entre 180 e 360 dias",">360 dias"))
		lineageTag: 25010980-9246-4da2-a198-b837bf12d26d
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	partition 'Slow Moving AXIA' = calculated
		mode: import
		source =
				
				
				SUMMARIZECOLUMNS('Estoque Atual AXIA'[COD_FILIAL],'Estoque Atual AXIA'[ID_PRODUTO],'Estoque Atual AXIA'[VLR_ESTOQUE],'Estoque Atual AXIA'[CUSTO_TOTAL],FILTER('Estoque Atual AXIA','Estoque Atual AXIA'[VLR_ESTOQUE]>0))

	annotation PBI_Id = a2a0598c0b024f359e2155abe1f55449

