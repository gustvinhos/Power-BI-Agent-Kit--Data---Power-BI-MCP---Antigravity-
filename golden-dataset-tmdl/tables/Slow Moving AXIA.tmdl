table 'Slow Moving AXIA'
	lineageTag: bd9110b5-ea2c-4126-8061-094c9c63ca5c

	measure 'Data da Última Compra (Medida)' =
			
			
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			    CALCULATE(
			        MAX('Entradas AXIA'[DATA_ENTRADA]),
			        FILTER(
			            'Dim CFOP',
			            'Dim CFOP'[CLASSIFICACAO_GERENCIAL] IN {
			                "E2 - Compra mercadoria",
			                "E4 - Simples remessa",
			                "E5 - Transferência",
			                "E6 - Bonificação"
			            }
			        )
			    )
			)
		formatString: Short Date
		lineageTag: 32ae21c3-dc51-494d-8185-dd923f1c5b22

	measure 'Data da Última Venda (Medida)' =
			
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			    CALCULATE(
			        MAX('Vendas Total AXIA'[DATA_EMISSAO])
			    )
			)
		formatString: Short Date
		lineageTag: 151415b1-c4e2-4f53-b9ac-2c7b663b2308

	measure 'Dias Sem Movimentação (Medida)' =
			
			VAR UltimaCompra = [Data da Última Compra (Medida)]
			VAR UltimaVenda  = [Data da Última Venda (Medida)]
			RETURN
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			    IF(
			        ISBLANK(UltimaCompra),
			        0,
			        MIN(
			            TODAY() - UltimaCompra,
			            TODAY() - UltimaVenda
			        )
			    )
			)
		formatString: 0
		lineageTag: 81ec890a-6065-4ab5-b253-868ebf4c31b8

		annotation PBI_FormatHint = {"isDecimal":true}

	measure 'Obsolescência (Medida)' =
			
			
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			IF([Dias Sem Movimentação (Medida)]<270,"Não Obsoleto","Obsoleto")
			)
		lineageTag: a021bd89-e857-4303-add6-55774185d208

	measure 'Classificação Slow Moving (Medida)' =
			
			
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			IF([DIO por Loja]<180,"<180 dias",IF(AND([DIO por Loja]>180,[DIO por Loja]<360),"Entre 180 e 360 dias",">360 dias"))
			)
		lineageTag: f7ead586-a5d6-41e1-96b8-fd2692883dd3

	measure 'R$ Vendas CMV 12 meses' =
			
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			CALCULATE(
			    SUM('Vendas Total AXIA'[VLR_CUSTO_CMV]),
			    FILTER(
			        'Vendas Total AXIA',
			        'Vendas Total AXIA'[FLG_REAL_SAIDA] = "REAL SAÍDA" &&
			        'Vendas Total AXIA'[DATA_EMISSAO] >= TODAY() - 365 &&
			        'Vendas Total AXIA'[DATA_EMISSAO] <= TODAY()
			    )
			)
			)
		formatString: "R$"\ #,0.00;-"R$"\ #,0.00;"R$"\ #,0.00
		lineageTag: 5143b6ce-daba-49f9-bf9b-52bd54252f12

		annotation PBI_FormatHint = {"currencyCulture":"pt-BR"}

	measure 'QTD Vendas 12 meses' =
			
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			CALCULATE(
			    SUM('Vendas Total AXIA'[QTD_AJUSTADA_OUTLIER]),
			    FILTER(
			        'Vendas Total AXIA',
			        'Vendas Total AXIA'[FLG_REAL_SAIDA] = "REAL SAÍDA" &&
			        'Vendas Total AXIA'[DATA_EMISSAO] >= TODAY() - 365 &&
			        'Vendas Total AXIA'[DATA_EMISSAO] <= TODAY()
			    )
			)
			)
		lineageTag: a211b1b7-1096-40e5-a8e6-6aeff44b3f61

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Data da Última Compra (Geral)' =
			
			
			IF(
			    CALCULATE( MAX('Slow Moving AXIA'[VLR_ESTOQUE]) ) = 0,
			    BLANK(),
			    CALCULATE(
			        MAX('Entradas AXIA'[DATA_ENTRADA])
			        )
			    )
		formatString: dd/mm/yyyy
		lineageTag: 9df3ff33-514b-4e69-bf74-6d7964dfbcc2

		annotation PBI_FormatHint = {"isDateTimeCustom":true}

